<Grid>
   <Cfg id="Zoom" NoVScroll="1" IdChars="0123456789" NumberId="1" Undo="1"/>
   <Cfg DefaultDate="1/1/2000 8:00"/>
   <Cfg ScrollLeftLap="0"/>
   <Actions OnDblClickGantt="ZoomToBar"/>
   <LeftCols>
      <C Name="id" Type="Int" CanEdit="0" CanSort="0"/>
      <C Name="START" Type="Date" Format="MMM dd, '&lt;span style=&quot;color:blue&quot;>'HH'&lt;/span>'" EditFormat="MM/dd/yy HH"/>
      <C Name="END" Type="Date" DefaultDate="1/1/2000 17:00" Format="MMM dd, '&lt;span style=&quot;color:blue&quot;>'HH'&lt;/span>'" EditFormat="MM/dd/yy HH"/>
      <C Name="DUR" Type="Text" CanSort="0"/>
      <C Name="ES" Type="Date" Visible="0"/>
      <C Name="LS" Type="Date" Visible="0"/>
      <C Name="EF" Type="Date" Visible="0"/>
      <C Name="LF" Type="Date" Visible="0"/>
   </LeftCols>
   <Cols>
      <C Name="GANTT" Type="Gantt" 
         GanttDataUnits="h"
         GanttDataModifiers="w:40,d:8,h:1"
         GanttStart="START" GanttEnd="END" GanttDuration="DUR"
         GanttMinStart="ES" GanttMaxStart="LS" GanttMinEnd="EF" GanttMaxEnd="LF"
         GanttLeft="0" GanttRight="0"
         GanttZoom="weeks"
         GanttSmoothZoom="1"
         GanttHideExclude="0" GanttCheckExclude="1"
         GanttExclude="d#17:00~8:00;d#12:00~13:00#1;w#12/11/1999~12/13/1999#3"
         />
   </Cols>
   <Zoom>
      <Z Name="weeks" 
         GanttUnits="d" GanttChartRound="w"
         GanttWidth="17" GanttWidthEx="71.4"
         GanttHeader1="M#MMMM yyyy"
         GanttHeader2="w#'week 'ddddddd" 
         GanttHeader3="d#%d"
         GanttHeader4="d#ddddd"
         GanttBackground="w#12/12/1999#5;M#1/1/2000~1/1/2000 4:00#4"
         GanttBackgroundEx="w#12/12/1999#5;M#1/1/2000~1/1/2000#4"
         />
      <Z Name="days" 
         GanttUnits="h" GanttChartRound="w" 
         GanttWidth="3" GanttWidthEx="9"
         GanttHeader1="d#dd MMM yyyy" 
         GanttHeader2="d#dddd" 
         GanttHeader3="h6#HH" GanttHeaderRound3="1"
         GanttBackground="w#12/12/1999#5;M#1/1/2000~1/1/2000 1:00#4"
         GanttBackgroundEx="w#12/12/1999#5;M#1/1/2000~1/1/2000#4"
         />
      <Z Name="hours" 
         GanttUnits="h" GanttChartRound="d"
         GanttWidth="19" GanttWidthEx="27.2"
         GanttHeader1="h12#dddd dddddd MMMM yyyy tt" GanttHeader1Ex="d#dddd dddddd MMMM yyyy"
         GanttHeader2="h#HH"
         GanttBackground="d#1/1/2000#4;w#12/12/1999~12/12/1999 0:05#5"
         GanttBackgroundEx="d#1/1/2000#4;w#12/12/1999#5"
         />
   </Zoom>
   <Body>
      <B>
         <I id="0"/>
         <I id="1" 
            START="12/16/1999 8:00" END="1/14/2000 17:00"
            ES="12/14/1999 8:00" LS="12/21/1999 8:00"
            EF="1/6/2000 17:00" LF="1/14/2000 17:00"
            />
         <I id="2" 
            START="12/31/1999 10:00" DUR="1h"
            />
         <I id="3" 
            START="12/16/1999 13:00" DUR="1.5w"
            />
         <I id="4" 
            START="1/10/2000 8:00"
            />
         <I id="5" 
            END="1/10/2000 17:00"
            />
         <I id="6" 
