            />
         <I id="3"
            CAL="Night shift"
            START="12/16/1999 9:00" END="1/3/2000 18:00"
            />
         <I id="4"
            CAL="24 hours"
            START="12/16/1999 9:00" END="1/3/2000 18:00"
            />
         <I id="5"
            CAL="Workdays"
            START="12/16/1999 9:00" END="1/3/2000 18:00"
            />
         <I id="6"
            CAL="Work hours only"
            START="12/16/1999 9:00" END="1/3/2000 18:00"
