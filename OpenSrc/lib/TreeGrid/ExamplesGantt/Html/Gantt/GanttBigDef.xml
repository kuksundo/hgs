<Grid>
   <Cfg id="GanttBig" SuppressCfg="1"/> <!-- Base settings, suppresses saving configuration to cookies -->
   <Cfg NumberId="1" IdChars="0123456789"/> <!-- Controls generation of new row ids -->
   <Cfg NoFormatEscape="1"/> <!-- You can use HTML code in formatting, set here because ValueSeparator and RangeSeparator contain HTML code -->
   <Cfg Sort="TASK,SECTION"/> <!-- Default sort is by Task and Section -->
   <Cfg Group="TASK"/> <!-- The grid is grouped by Task by default -->
   <Cfg GroupRestoreSort="1"/> <!-- Restores original sorting after ungroup -->
   <Cfg Undo="1"/> <!-- Permits undo / redo changes by Ctrl+Z / Ctrl + Y -->
   <Cfg ChildParts="2"/> <!-- Switches on child parts -->
   <Cfg ScrollLeftLap="0"/> <!-- Permits saving left scroll to cookies -->
   <Cfg DefaultDate="1/1/2010"/> <!-- Default date in calendar for empty date -->
   <Cfg MenuColumnsCount="2"/> <!-- Displays column names in menu in two columns -->
   <Cfg MidWidth="295" MinRightWidth="200" MinLeftWidth="200"/> <!-- Sizes and scrolls for column sections -->
   <Cfg Style="Standard"/> <!-- Presets Standard CSS style -->
   <Cfg Paging="2" PageLength="5"/> <!-- Sets paging and presets page length -->
   <Cfg MaxHeight="1" MinTagHeight="450"/> <!-- Maximizes grid to the page without scrollbars, minimal height 450px -->
   <Cfg CalendarsRepeatType="Enum" CalendarsRepeatEnum="||Weekly|Daily" CalendarsRepeatEnumKeys="||w|d"/> <!-- Settings for Calendar list dialog, the Repeat column -->
   <Cfg CalendarsValueType="Enum" CalendarsValueEnum="|Not work|Exception|Lunch|Weekend|Holiday|Reserved1|Reserved2" CalendarsValueEnumKeys="||0|1|2|3|4|5" CalendarsValueCaption="Type"/> <!-- Settings for Calendar list dialog, the Value column -->
   <Def>
      <D Name="R" CDef=""/> <!-- The standard rows cannot have children -->
      <D Name="Group" Calculated="1" CalcOrder="SECTIONHtmlPostfix,START,END,DURATION,COMPLETE,PARTS1,PRICE"/> <!-- Grouping row is calculated to show summary for its children -->
      <D Name="Group" Expanded="1" /> <!-- Grouping row shows its children by default -->
      <D Name="Group" EditCols="Main"/> <!-- When user changes value in main column (N1 - Task), the value is copied also to the children -->
      <D Name="Group" STARTFormula="ganttstart()"/> <!-- Gets the first start date from its children, including milestones -->
      <D Name="Group" ENDFormula="ganttend()"/> <!-- Gets the last end date from its children, including milestones -->
      <D Name="Group" DURATIONFormula="END&amp;&amp;START?Grid.DiffGanttDate(END,START):''"/> <!-- Gets the last end date from its children, including milestones -->
      <D Name="Group" COMPLETEFormula="ganttpercent()"/> <!-- Calculates average task completion from its children -->
      <D Name="Group" PARTS1Formula="sumparts()" PARTS1CanEdit="0" GANTTGanttClass1="White"/> <!-- Merges all date ranges from its children -->
      <D Name="Group" PRICEFormula="sum()"/> <!-- Sums prices -->
      <D Name="Group" CDef="R"/> <!-- Grouping rows can contain standard rows as children -->
      <D Name="Group" GANTTGanttClass="Group" GANTTGanttIcons="1"/> <!-- Gantt setting specific for Group rows, changes colors and adds side icons -->
      <D Name="Group" GANTTGanttSummary="0"/> <!-- Row works as Gantt Summary, when moved, it moves its child tasks too -->
      <D Name="Group" GANTTGanttEdit="Main"/> <!-- Only main the bar can be edited to move all the summary children -->
      <D Name="Group" STARTCanEdit="1"/> <!-- Lets users to edit start date to move the Gantt Summary and its children -->
      <D Name="Group" GroupMain="TASK"/> <!-- Tree will be shown in Task column -->
      <D Name="Group" CMaxChars="100"/> <!-- Ensures setting returned value when grouped by Complete column -->
      <D Name="Group" ParentCDef="Group"/> <!-- When grouped by any column, new rows added to root will be these groups -->
      <D Name="Group" CanFilter="2"/> <!-- Hides the group when all its tasks are hidden by filter -->
      <D Name="Group" idVisible="0"/> <!-- Hides the automatically generated id -->
      <D Name="Group" CanSelect="0"/> <!-- Hides the panel Select icon -->
      <D Name="Group" CALENDARVisible="-1"/> <!-- Custom calendar has no sense for group tasks -->
      
      <D Name="Group" SECTIONHtmlPostfixFormula="' &lt;span style=&quot;color:green;&quot;>('+count(4)+')&lt;/span>'"/> <!-- Shows the count of children as green suffix -->

      <D Name="GroupTask" GroupCols="|TASK|COMPLETE,TASK" Group="1" Def="Group" GroupMain="SECTION"/> <!-- When grouping by Task, shows tree in Section column -->
      <D Name="GroupTask" ParentCDef="GroupTask"/> <!-- When grouped by task, new rows added to root will be of these groups -->
   </Def>
   <LeftCols>
      <C Name="id" Type="Int" Width="25" WidthPad="17" CanEdit="0"/> <!-- id / row number column -->
      <C Name="TASK" Width="70" Type="Text"/> <!-- Task column -->
      <C Name="SECTION" Width="80" Type="Text"/> <!-- Section column -->
   </LeftCols>
   <Cols>
      <C Name="START" Width="65" Type="Date" Format="MMM dd" EditFormat="M/d"/> <!-- Start date column, 1st plan -->
      <C Name="END" Width="65" Type="Date" Format="MMM dd" EditFormat="M/d" CanEmpty="1"/> <!-- End date column, 1st plan -->
      <C Name="DURATION" Width="31" Type="Text" Align="Right" MenuName="Duration"/> <!-- Duration column, 1st plan, does not have data, calculated from START and END -->
      <C Name="COMPLETE" Width="35" Type="Float" EditMask="^\d*[\d\.\,]?\d*$" Format="##.##\%;;0\%" MenuName="Complete"/> <!-- Complete column, 1st plan -->
      <C Name="TEXT" Visible="0" Width="120"/> <!-- Main bar text, 1st plan -->

      <C Name="DESCENDANTS" Width="50" Type="Text" Range="1" MenuName="Descendants"/><!-- Descendants, ids of next tasks -->
      <C Name="ANCESTORS" Width="50" Type="Text" Range="1" MenuName="Ancestors"/><!-- Ancestors, ids of previous tasks -->
      <C Name="SLACK" Width="40" Type="Float" CanEmpty="1" CanEdit="0" MenuName="Slack"/> <!-- Slack for critical path, automatically filled -->

      <C Name="CALENDAR" Visible="0" Width="70" Type="Select" OnClickSideDefaults = "Grid.ShowGanttCalendars(Row,Col)" OnEnter = "Grid.ShowGanttCalendars(Row,Col)"/> <!-- Custom calendar / excluded dates -->

      <C Name="RESOURCES" Width="110" Type="Text"/> <!-- Resources column -->
      <C Name="PRICE" Width="55" Type="Float" Format=",0.00" Formula="Grid.GetGanttPrice(Row,'GANTT')"/> <!-- Task price column -->

      <C Name="PARTS1" Width="150" Type="Date" DefaultDate="5/1/2008 9:00~5/1/2008 18:00" Range="1" Button="" Format="MMM dd, '&lt;span style=&quot;color:blue&quot;>'HH'&lt;/span>'"/> <!-- Main bar Parts, 2nd plan -->
      <C Name="TEXT1" Visible="0" Width="70"/> <!-- Main bar text, 2nd plan -->

      <C Name="FLAGS" Visible="0" Width="75" Type="Date" Range="1" Button="" Format="MMM dd, '&lt;span style=&quot;color:blue&quot;>'HH'&lt;/span>'"/> <!-- Flag column -->
      <C Name="FLAGSTEXT" Visible="0" Width="60" Type="Text" Range="1"/> <!-- Flag info column -->

      <C Name="MINSTART" Visible="0" Width="80" Type="Date" Format="MMM dd, '&lt;span style=&quot;color:blue&quot;>'HH'&lt;/span>'" EditFormat="MM/dd/yy HH"/> <!-- Min Start date column -->
      <C Name="MAXSTART" Visible="0" Width="80" Type="Date" Format="MMM dd, '&lt;span style=&quot;color:blue&quot;>'HH'&lt;/span>'" EditFormat="MM/dd/yy HH"/> <!-- Max Start date column -->
      <C Name="MINEND" Visible="0" Width="80" Type="Date" Format="MMM dd, '&lt;span style=&quot;color:blue&quot;>'HH'&lt;/span>'" EditFormat="MM/dd/yy HH"/> <!-- Min End date column -->
      <C Name="MAXEND" Visible="0" Width="80" Type="Date" Format="MMM dd, '&lt;span style=&quot;color:blue&quot;>'HH'&lt;/span>'" EditFormat="MM/dd/yy HH"/> <!-- Max End date column -->
      
   </Cols>

   <!-- Column captions and header tooltips -->
   <Header 
      id="ID" idTip="id - Task unique idenfication&lt;br>Not used for automatically created grouped tasks&lt;br>Used especially to assign dependencies to tasks and to identify tasks when saving changes to server"
      TASK="Task" TASKTip="Task name, the same for more rows, any custom string"
      SECTION="Section" SECTIONTip="Name of individual task part, any custom string&lt;br>When grouped, it shows task name in parent and the section name in child row"
      START="Start" STARTTip="Start date (and time) of the task, first plan"
      END="End" ENDTip="End date (and time) of the task, first plan"
      COMPLETE="Com&lt;br>plete" COMPLETETip="Percentage completion of the task, first plan&lt;b>By default only not started tasks (0%) can be scheduled"
      DURATION="Dura&lt;br>tion" DURATIONTip="Duration in hours of the task, first plan"
      TEXT="Information" TEXTTip="Task custom information, here shown in task tip, first plan"
      PARTS1="Parts 1" PARTS1Tip="Start and End dates of individual parts, if the task is discrete, second plan"
      TEXT1="Information 1" TEXT1Tip="Task custom information, here shown in task tip, second plan"
      DESCENDANTS="Descen&lt;br>dants" DESCENDANTSTip="(Successors) Dependencies to next tasks, their ids, optionally with lag, span and color, here used for task first plan"
      ANCESTORS="Ances&lt;br>tors" ANCESTORSTip="(Predecessors) Dependencies from previous tasks, their ids, optionally with lag, span and color, here used for task first plan"
      SLACK="Slack&lt;br>(critical path)" SLACKTip="Calculated value for task first plan, in hours&lt;br>How much the task can be delayed to not affect the project end date (or start date)&lt;br>If it is 0, the task is on critical path&lt;br>If it is negative, the task is incorrect"
      CALENDAR="Calendar" CALENDARTip="Custom calendar for all task plans and other Gantt objects.&lt;br>Defines excluded dates ingored in Gantt calculations"
      RESOURCES="Resources" RESOURCESTip="Resources assigned to task, first plan&lt;b>Used to calculate task price and to show project resources usage in the grid bottom"
      PRICE="Price" PRICETip="Price of the task, first plan. Calculated from the resources assigned to task and the task duration"
      MINSTART="Min start" MINSTARTTip="Early start constraint, here used for the task first plan"
      MAXSTART="Max start" MAXSTARTTip="Late start constraint, here used for the task first plan"
      MINEND="Min end" MINENDTip="Early end constraint, here used for the task first plan" 
      MAXEND="Max end" MAXENDTip="Late end constraint, here used for the task first plan"
      FLAGS="Flags" FLAGSTip="Icon on the selected date, for custom information for the date"
      FLAGSTEXT="Flag Info" FLAGSTEXTTip="Custom information for the flag date"
      GANTT="Gantt"/>
   <Header SortIcons="2"/> <!-- Hides unused sort icons -->   
   <Header NoEscape="1"/> <!-- The header cells contains HTML code -->   

   <RightCols>
      <!-- Gantt chart column -->

      <!-- Basic settings -->
      <C Name="GANTT" Type="Gantt" MenuName="Gantt chart"/> <!-- Basic setting, type and name in columns menu -->
      <C Name="GANTT" GanttCount="2" GanttTask="Main"/> <!-- Defines two main bar plans for this chart, but only the first one will be Gantt task with dependencies, resources and percentage completion -->

      <!-- Data settings -->
      <C Name="GANTT" GanttDataUnits="d" /> <!-- All lengths in input data XML (like Dependency lags) are in days -->
      <C Name="GANTT" GanttDataModifiers="m:1/60,h:1,d:8,w:40"/> <!-- Modifiers that can be used in Dependency lag values to multiply the value to get hour count -->
      <C Name="GANTT" GanttLastUnit="d"/> <!-- All the end dates are set as the last unit (day) -->

      <!-- First main bar plan definition -->
      <C Name="GANTT" GanttStart="START" GanttEnd="END" GanttDuration="DURATION" GanttComplete="COMPLETE" GanttText="TEXT"/> <!-- Defines source columns for Main tasks - first main bar plan -->
      <C Name="GANTT" GanttResources="RESOURCES"/> <!-- Resources assigned to the first main bar plan -->
      <C Name="GANTT" GanttHtmlRight="$&lt;span style='color:#77F'>*PRICE*&lt;/span> *RESOURCES*"/> <!-- Formats the text next to main task -->

      <!-- Second main bar plan definition -->
      <C Name="GANTT" GanttParts1="PARTS1" GanttText1="TEXT1"/> <!-- Defined source columns for the second main bar plan -->
      <C Name="GANTT" GanttTop1="4" GanttHeight1="1"/> <!-- Vertical position and height of the second main bar plan -->
      <C Name="GANTT" GanttClass1="Yellow"/> <!-- The second main bar plan has set yellow background -->
      <C Name="GANTT" GanttMilestones1="0"/> <!-- Restricts milestones for the second main bar plan -->

      <!-- Flags definition -->
      <C Name="GANTT" GanttFlags="FLAGS" GanttFlagTexts="FLAGSTEXT" GanttFlagsMove="2"/> <!-- Defined source columns for Flags, can move flags between rows -->

      <!-- Vertical lines definition -->
      <C Name="GANTT" GanttLines="3#1/20/2010#Navy2"/> <!-- Defines one vertical line for simulated actual date -->
      <C Name="GANTT" GanttLine0Tip="Actual date for this project is at *Start*"/> <!-- Tip for the simulated actual date -->

      <!-- Settings for scheduling -->
      <C Name="GANTT" GanttDescendants="DESCENDANTS" GanttAncestors="ANCESTORS"/> <!-- Defines source columns for Dependencies -->
      <C Name="GANTT" GanttMinStart="MINSTART" GanttMaxStart="MAXSTART" GanttMinEnd="MINEND" GanttMaxEnd="MAXEND"/> <!-- Start and end date constraints -->
      <C Name="GANTT" GanttSlack="SLACK"/> <!-- Where will be filled information for critical path -->
      <C Name="GANTT" GanttAssignDependencies="3"/> <!-- Assigns different colors to dependencies to the same box and its edge -->
      <C Name="GANTT" GanttAllDependencies="3"/> <!-- Removes dependency lines if both tasks are hidden -->
      <C Name="GANTT" GanttStrict="1"/> <!-- Forces first tasks to start on base date and all other to start immediately when possible -->
      <C Name="GANTT" GanttDirection="0"/> <!-- Schedules tasks from start -->
      <C Name="GANTT" GanttCorrectDependencies="0" /> <!-- Ignore dependencies after main task move or resize or dependency change -->
      <C Name="GANTT" GanttCorrectDependenciesFixed="1"/> <!-- Cannot move the changed task when automatically correcting dependencies -->
      <C Name="GANTT" GanttCheckExclude="2"/> <!-- Ask to move task if it starts on holiday after resize or move -->
      <C Name="GANTT" GanttBase="1/4/2010"/> <!-- Base project date, all the project should start after this date -->
      <C Name="GANTT" GanttFinish=""/> <!-- Project end date, all the tasks should end before this date. Not defined by default here -->
      <C Name="GANTT" GanttBaseProof="1"/> <!-- The tasks cannot be moved or created before GanttBase -->
      <C Name="GANTT" GanttFinishProof="1"/> <!-- The tasks cannot be moved or created after GanttFinish, if defined -->
      <C Name="GANTT" GanttBaseTip="Project start set at *Start*" GanttBaseAutoTip="Project starts at *Start*"/> <!-- Tip for GanttBase line, if defined and if not defined -->
      <C Name="GANTT" GanttFinishTip="Project finish set at *Start*" GanttFinishAutoTip="Project finishes at *Start*"/> <!-- Tip for GanttFinish line, if defined and if not defined -->
      <C Name="GANTT" GanttLineTipDateFormat="&lt;b>dddddd MMMM&lt;/b>"/> <!-- Sets date format for all lines tips, for GanttBase and GanttFinish -->

      <!-- Resources -->
      <C Name="GANTT" GanttResourcesAssign="1"/> <!-- Shows resources menu with checkboxes -->

      <!-- Tooltips -->
      <C Name="GANTT" GanttTip="*id*. *TASK*, *SECTION* &lt;span style='color:#F77'>(first bar)&lt;/span>&lt;div style='padding-top:5px;padding-bottom:5px;font-weight:bold;'>*Start* - *End*&lt;/div>*Duration* hours, *COMPLETE* completed&lt;div style='padding-top:5px;'>&lt;b>*SLACK*&lt;/b> hours reserve&lt;/div>&lt;div style='padding-top:5px;'>*Text*&lt;/div>"/> <!-- Specifies tooltip shown for first main bar -->
      <C Name="GANTT" GanttMilestoneTip="*id*. *TASK*, *SECTION* &lt;span style='color:#F77'>(milestone)&lt;/span>&lt;div style='padding-top:5px;font-weight:bold;'>*Start*&lt;/div>&lt;div style='padding-top:5px;'>&lt;b>*SLACK*&lt;/b> hours reserve&lt;/div>&lt;div style='padding-top:5px;'>*Text*&lt;/div>"/> <!-- Specifies tooltip shown for first main plan milestone-->
      <C Name="GANTT" GanttTip1="*id*. *TASK*, *SECTION* &lt;span style='color:#F77'>(*Index*. second bar)&lt;/span>&lt;div style='padding-top:5px;padding-bottom:5px;font-weight:bold;'>*Start* - *End*&lt;/div>*Duration* hours&lt;div style='padding-top:5px;'>*Text*&lt;/div>"/> <!-- Specifies tooltip shown for second main bar -->
      <C Name="GANTT" GanttFlagsTip="*Start* - *Text*"/> <!-- Specifies tooltip shown for flags -->
      <C Name="GANTT" GanttDependencyTip="From: &lt;b>*From*&lt;/b>, *Start*&lt;br>To: &lt;b>*To*&lt;/b>, *End*&lt;br>Type: &lt;b>*Type*&lt;/b>&lt;br>Lag: &lt;b>*Lag*&lt;/b>&lt;br>Float: &lt;b>*Float*&lt;/b>" GanttDependencyTipDateFormat="dddddd MMMM"/> <!-- Specifies tooltip shown for dependency line -->
      
      <!-- Exclude settings -->
      <C Name="GANTT" GanttCalendar=""/> <!-- Column with custom calendars, by default is turned off -->
      <C Name="GANTT" GanttExclude="Weekends"/> <!-- Global calendar - the excluded dates are not used for Gantt calculations and behaves like they don't exist at all -->
      <C Name="GANTT" GanttHideExclude="0"/> <!-- By default are the Excluded dates in global calendar shown in chart, it can be changed by a user -->
      <C Name="GANTT" GanttAvailabilityExclude="2"/> <!-- The resources chart is rounded to the excluded dates -->

      <!-- Horizontal Gantt paging settings -->
      <C Name="GANTT" GanttPaging="1"/> <!-- Permits Gantt paging -->
      <C Name="GANTT" GanttHeaderPaging2="1" GanttHeaderPaging3="1"/> <!-- The second and third headers will be paged, the first header will be shown always the whole -->

      <!-- Basic zoom settings -->
      <C Name="GANTT" GanttZoom="weeks and days"/> <!-- Predefine zoom level name -->
      <C Name="GANTT" GanttChartMinStart="1/1/1990" GanttChartMaxEnd="1/1/2040"/> <!-- Limit dates for input and display -->
      <C Name="GANTT" GanttLeft="2" GanttRight="0"/> <!-- At least two units will be let empty on left side (no Exclude applied here) -->
      <C Name="GANTT" MinWidth="450"/> <!-- Minimal width of the column will be 450 pixels -->
      <C Name="GANTT" GanttBackground="M#1/1/2008#5"/> <!-- Highlights these dates in chart (month ends), some zoom levels change this setting -->
   
      <!-- Gantt Zoom defines zoom levels definition -->
      <!-- The individual levels predefine various Gantt zoom settings -->
      <!-- GanttUnits and GanttWidth specify the zooming size, to GanttUnits are all the dates rounded for display and drag -->
      <!-- GanttChartRound specifies rounding of first and last date in the chart -->
      <!-- Some zoom levels changes GanttBackground to mark different units -->
      <!-- GanttHeader1 specifies the dates shown in the first line in Gantt header -->
      <!-- GanttHeader2 and possibly also 3,4,5 specify next lines in Gantt header -->
      <!-- The ...Ex values are chosen when Exclude is hidden, they are appropriate only for the actual Exclude dates (days and hours) -->
      <C Name="GANTT" GanttZoomList="[
            { Name:'years and halves', GanttUnits:'M6', GanttChartRound:'y', GanttWidth:18, GanttWidthEx:25.2,
            GanttBackground:';y#1/1/2008',
            GanttHeader1:'y#yyyy', 
            GanttHeader2:'M6#MMMMMM' },

            { Name:'years and quarters', GanttUnits:'M3', GanttChartRound:'y', GanttWidth:24, GanttWidthEx:28.8,
            GanttBackground:';y#1/1/2008',
            GanttHeader1:'y#yyyy', GanttHeader2:'M3#MMMMM' },

            { Name:'halves and months', GanttUnits:'M', GanttChartRound:'y', GanttWidth:18, GanttWidthEx:25.2, 
            GanttBackground:';M6#1/1/2008',
            GanttHeader1:'M6#MMMMMM. yyyy', 
            GanttHeader2:'M#MM' },

            { Name:'quarters and months', GanttUnits:'M', GanttChartRound:'M6', GanttWidth:28, GanttWidthEx:39.2,
            GanttBackground:';M3#1/1/2008',
            GanttHeader1:'M3#MMMMM. yyyy', 
            GanttHeader2:'M#MMM' },

            { Name:'months and weeks', GanttUnits:'d', GanttChartRound:'M', GanttWidth:3, GanttWidthEx:4.2,
            GanttBackground:';M#1/1/2008',
            GanttHeader1:'M#MMM yyyy', 
            GanttHeader2:'w#d.' },
   
            { Name:'months and days', GanttUnits:'d', GanttChartRound:'M', GanttWidth:8, GanttWidthEx:11.2,
            GanttHeader1:'M#MMMM yyyy', 
            GanttHeader2:'d#&quot;&lt;div style=\'font:8px Arial;\'>&quot;DDDDDD&quot;&lt;br/>&quot;DDDDDDD&quot;&lt;/div>&quot;', GanttHeaderHeight2:'20' },
   
            { Name:'weeks and days', GanttUnits:'d', GanttChartRound:'w', GanttWidth:18,  GanttWidthEx:25.2,
            GanttHeader1:'w#&quot;&lt;span style=\'color:red;font-size:8px;\'>week &quot;ddddddd&quot;&lt;/span>&quot; MMMM yyyy', 
            GanttHeader2:'d#%d',
            GanttHeader3:'d#ddddd' },

            { Name:'days and quarters', GanttUnits:'h', GanttChartRound:'w', GanttWidth:3,
            GanttHeader1:'d#dd MMM yyyy', 
