  If LunarYear <> 1881 Then Begin
    m1 := LunarYear - 1882 + 1;
    For i := 1 To m1 Do Begin
      For j := 1 To 13  Do DayTotal := DayTotal + kk[i][j];
      If kk[i][13] = 0 Then DayTotal := DayTotal + 336
       Else DayTotal := DayTotal + 362
      end;
    end;

